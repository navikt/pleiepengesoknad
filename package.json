{
    "name": "pleiepengesoknad",
    "version": "1.0.2",
    "description": "Selvbetjeningsløsning for pleiepengesøknad",
    "repository": "https://github.com/navikt/pleiepengesoknad",
    "license": "MIT",
    "author": "NAV",
    "scripts": {
        "build": "node src/build/scripts/production-build.js",
        "dev-api": "node server/api-mock.js",
        "sam": "node server/api-mock.js",
        "start-api-mock": "node server/api-mock.js",
        "dev": "npm-run-all -p start-api-mock start-no-decorator",
        "start": "node ./src/build/scripts/start-dev.js",
        "snd": "node ./src/build/scripts/start-dev-no-decorator.js",
        "start-no-decorator": "node ./src/build/scripts/start-dev-no-decorator.js",
        "start-express": "node ./server.js",
        "test": "jest --maxWorkers=2 --config ./jest/jest.config.js",
        "snyk-protect": "snyk protect",
        "prepare": "npm run snyk-protect",
        "cypress:install": "npm add -D cypress cypress-file-upload",
        "cypress:open": "CYPRESS_baseUrl=http://localhost:8080 cypress open --project $PWD/e2e",
        "cypress:run": "CYPRESS_baseUrl=http://localhost:8080 cypress run --project $PWD/e2e",
        "cypress:ci": "CYPRESS_baseUrl=http://localhost:8080 cypress run --project $PWD/e2e --reporter junit --reporter-options \\\"mochaFile=results/my-test-output.xml\\\"",
        "cypress:headless": "docker run -e CYPRESS_video=false -e CYPRESS_baseUrl=http://host.docker.internal:8080 -it -v $PWD/e2e:/e2e -w /e2e cypress/included:4.0.1",
        "e2e": "API_URL=http://host.docker.internal:8082 npm-run-all -p dev -r cypress:headless",
        "e2e-docker-compose": "docker-compose -f ./e2e/docker-compose.yml up --build --exit-code-from e2e",
        "e2e-docker-rm": "docker rm $(docker ps -a -f status=exited -q) ",
        "e2e-docker-rmi": "docker images -a | grep e2e | awk '{print $3}' | xargs docker rmi",
        "server-test": "node ./server-test.js"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "dependencies": {
        "@formatjs/intl-pluralrules": "^1.5.9",
