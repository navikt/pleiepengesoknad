        "build-heroku": "node src/build/scripts/heroku-build.js",
        "dev-api": "PORT=8082 node ./api-mock.js",
        "dev": "npm-run-all -p start-api-mock start-no-decorator",
        "start": "node ./src/build/scripts/start-dev.js",
        "snd": "node ./src/build/scripts/start-dev-no-decorator.js",
        "sam": "PORT=8082 node ./api-mock.js",
        "start-no-decorator": "node ./src/build/scripts/start-dev-no-decorator.js",
        "start-express": "node ./server.js",
        "start-api-mock": "PORT=8082 node ./api-mock.js",
        "test": "jest --maxWorkers=2 --config ./jest/jest.config.js",
        "snyk-protect": "snyk protect",
        "prepare": "npm run snyk-protect",
        "cypress:open": "npm add -D cypress && CYPRESS_baseUrl=http://localhost:8080 cypress open",
        "cypress:run": "npm add -D cypress && CYPRESS_baseUrl=http://localhost:8080 cypress run",
        "cypress:ci": "CYPRESS_baseUrl=http://localhost:8080 cypress run --reporter junit --reporter-options \"mochaFile=results/my-test-output.xml\"",
        "cypress:headless": "docker run --net=\"host\" -e CYPRESS_video=false -e CYPRESS_baseUrl=http://$(ipconfig getifaddr en0):8080 -it -v $PWD:/e2e -w /e2e cypress/included:3.2.0",
        "e2e": "npm-run-all -p start-api-mock start-no-decorator -r cypress:headless",
        "e2e-docker-compose": "docker-compose up --build --exit-code-from cypress"
    },
